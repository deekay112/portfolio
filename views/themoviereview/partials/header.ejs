<body>
    
    



<nav class="navbar navbar-default">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand" href="/tmr">The Movie Review</a>
    </div>

    <ul class="nav navbar-nav navbar-right">
        
    <% if(!newUser) { %>     
      <li><a href="/tmr/login"><span class="glyphicon glyphicon-user"></span> Login</a></li>
      <li><a href="/tmr/register"><span class="glyphicon glyphicon-log-in"></span> Sign Up</a></li>
    <% } else { %> 
      <li><a href="#"><span class="glyphicon glyphicon-user"></span> Logged in as <%=newUser.username%> </a></li>
      <li><a href="/tmr/logout"><span class="glyphicon glyphicon-log-out"></span> Sign Out</a></li>   
     <% } %>  
    </ul>
  </div>
</nav>









<div class="container">
    <% if (error && error.length > 0) { %>
    <div class="alert alert-danger" role="alert">
          <%= error %>
    </div>
    <% } %>
    <% if (success && success.length > 0) { %>
    <div class="alert alert-success" role="alert">
          <%= success %>
    </div>
    <% } %>
</div>